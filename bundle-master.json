{
	"name": "js-vue-utils",
	"version": "1.0-SNAPSHOT",
	"variables": {
		"github": "https://raw.githubusercontent.com/nablex/js-vue-utils",
		"version": "master"
	},
	"scopes": {
		"core": {
			"vue.js": {
				"description": "Slightly modified version of vuejs",
				"folder": "core",
				"url": "${github}/${version}/vue.js"
			}
		},
		"misc": {
			"render.js": {
				"description": "A reusable rendering function",
				"folder": "misc",
				"url": "${github}/${version}/misc/render.js",
			}
			"vue-router.js": {
				"description": "Contains a generic router",
				"folder": "misc",
				"url": "${github}/${version}/misc/vue-router.js",
				"requires": {
					"js-utils": {
						"type": "bundle",
						"url": "https://raw.githubusercontent.com/nablex/js-utils/master/bundle-${version}.json",
						"scopes": ["misc"],
						"includes": ["router.js"]
					},
					"vue.js": {
						"self": "core"
					},
					"render.js": {
						"self": "misc"
					}
				}
			}
		},
		"nabu": {
			"cms-masterdata.js": {
				"folder": "cms",
				"url": "${github}/${version}/cms/cms-masterdata.js",
			}
		}
	}
}
